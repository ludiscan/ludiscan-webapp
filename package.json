{
  "name": "ludiscan-webapp",
  "private": true,
  "engines": {
    "node": "23.6.x",
    "npm": "10.x"
  },
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "fix:eslint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:stylelint": "stylelint 'src/**/*.{tsx,css}'",
    "fix:stylelint": "stylelint --fix 'src/**/*.{tsx,css}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "lint": "run-p lint:eslint lint:stylelint",
    "fix": "run-p fix:eslint fix:stylelint",
    "test": "jest",
    "type": "tsc -noEmit",
    "preview": "vite preview",
    "release:patch": "npm version patch --message 'v%s' && git push && git push origin --tags",
    "release:minor": "npm version minor --message 'v%s' && git push && git push origin --tags",
    "release:major": "npm version major --message 'v%s' && git push && git push origin --tags",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:api-schemes": "npx openapi-typescript https://matuyuhi.com/api-json --output generated/api.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@react-three/drei": "^9.121.3",
    "@react-three/fiber": "^8.17.12",
    "@reduxjs/toolkit": "^2.5.1",
    "@tanstack/react-query": "^5.65.1",
    "@types/node": "^22.10.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-modal": "^3.16.3",
    "framer-motion": "^12.4.2",
    "openapi-fetch": "^0.13.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-modal": "^3.16.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.3",
    "redux": "^5.0.1",
    "three": "^0.172.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@react-three/eslint-plugin": "^0.1.2",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-vite": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/three": "^0.173.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all2": "^7.0.2",
    "openapi-typescript": "^7.6.0",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.4.2",
    "storybook": "^8.5.1",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^37.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
